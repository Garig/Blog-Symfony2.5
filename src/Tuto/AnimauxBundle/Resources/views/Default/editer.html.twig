{% extends "::base2.html.twig" %}

{% block title %}Editer article | Administration{% endblock %}

{% block body %}

<h1 class="index">Editer un article</h1>

<form method="post" action="{{ path("tuto_animaux_editer", {id: id}) }}" {{ form_enctype(form) }}>

	{{ form_row(form.titre, { 'attr': {'class': 'form-titre'} }) }}

	{{ form_row(form.description, { 'attr': {'class': 'ckeditor'} }) }}

	{{ form_row(form.url, { 'attr': {'class': 'form-url'} }) }}

	{{ form_row(form.categorie, { 'attr': {'class': 'form-titre'} }) }}

	{{ form_rest(form) }}

    <input type="submit" class="btn btn-primary">
</form>
        

{% endblock %}